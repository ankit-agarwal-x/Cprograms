#include<stdio.h>
 void swap1(char **str1_ptr, char **str2_ptr)
  {
     char *temp= *str1_ptr ;
     *str1_ptr = *str2_ptr ;
       *str2_ptr = temp ;
  }
   void main ()
   {
     char *str1= "fjhjj";
     char *str2= "eve";
      swap1(&str1 , &str2);
      printf("str1 is %s, str2 is %s", str1, str2);
    getchar();
     }
