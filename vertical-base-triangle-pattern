#include<stdio.h>
void main ()
{
	int n;
	printf("Enter the length \n");
	scanf("%d",&n);
	for (int i=1; i<=(n+1)/2; i++)
	{
		for (int j=0; j<i; j++)
		{
			printf("# ");
		}

		printf("\n");
	}
	for (int i=1; i<=(n-1)/2; i++ )
	{
		for(int j=(n-1)/2; j>=i; j--)
		{
			printf("# ");
		}
		printf("\n");
	}
}
